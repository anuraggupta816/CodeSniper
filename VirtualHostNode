<VirtualHost *:80>
    ServerAdmin api.yourfoodrunner.com
    ServerName api.yourfoodrunner.com
    ServerAlias api.yourfoodrunner.com

    ProxyRequests On
    <Proxy *>
        Order deny,allow
        Allow from all
    </Proxy>

    <Location />
        ProxyPass http://127.0.0.1:3001
        ProxyPassReverse http://127.0.0.1:3001
    </Location>
</VirtualHost>
